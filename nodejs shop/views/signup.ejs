<div class="container">
  <h2>회원가입</h2>
  <form action="/auth/signup_process" method="post">
    <div class="mb-3">
      <label class="form-label" for="id">아이디</label>
      <input class="form-control" type="text" name="id" id="id" style="width:300px;" />
    </div>
    <div class="mb-3">
      <label class="form-label" for="pwd">비밀번호</label>
      <input class="form-control" type="password" name="pwd" id="pwd" style="width:300px;" />
    </div>
    <div class="mb-3">
      <label class="form-label" for="name">이름</label>
      <input class="form-control" type="name" name="name" id="name" style="width:300px;" />
    </div>
    <div class="mb-3">
      <label class="form-label" for="address">주소</label>
      <input class="form-control" type="address" name="address" id="address" style="width:300px;" />
    </div>
    <div class="mb-3">
      <label class="form-label" for="tel">전화번호</label>
      <input class="form-control" type="tel" name="tel" id="tel" style="width:300px;" />
    </div>
    <div class="mb-3">
      <label class="form-label" for="birth">생년월일</label>
      <input class="form-control" type="birth" name="birth" id="birth" style="width:300px;" />
    </div>
    <button class="btn btn-outline-primary btn-sm" type="submit">회원가입</button>
  </form>
</div>